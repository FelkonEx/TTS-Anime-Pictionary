    height="605"
    padding="10 10 10 10"
    color="Black">
  </Panel>

<!-- Text -->
  <Text
    class="panelHeading"
    color="#ffffff"
    alignment="MiddleCenter"
    fontSize="75">
  </Text>

  <Text
    class="subheadingText"
    color="#99aab5"
    alignment="UpperMiddle"
    fontSize="45">
  </Text>

  <Text
    class="generalText"
    color="#ffffff"
    fontSize="30"
    alignment="MiddleLeft">
  </Text>

  <Text
    class="deckSizeText"
    color="clear"
    fontSize="30"
    alignment="MiddleRight">
  </Text>

  <Text
    class="showHideText"
    color="#ffffff"
    alignment="MiddleCenter"
    fontSize="30">
  </Text>

<!-- Row -->
  <Row
    class="subheadingRow"
    preferredHeight="55">
  </Row>

  <Row
    class="generalRow"
    preferredHeight="75">
  </Row>

  <Row
    class="separatorRow"
    preferredHeight="10"
    dontUseTableRowBackground="True"
    color="black">
  </Row>

<!-- Toggle -->
  <Toggle
    class="checkbox"
    scale="3 3"
    isOn="False"
    onValueChanged="addDeck">
  </Toggle>

<!-- Button -->
  <Button
    class="tileButton"
    padding="0 0 0 0"
    image="platelet_1"
    transition="SpriteSwap"
    highlightedSprite="platelet_2"
    pressedSprite="platelet_3">
  </Button>

  <Button
    class="settingsOff"
    active="True"
    preferredHeight="175"
    color="#9fa4a5"
    padding="25 0 0 0">
  </Button>

  <Button
    class="settingsOn"
    active="False"
    preferredHeight="175"
    color="#31b32b"
    padding="25 0 0 0">
  </Button>

  <Button
    class="generalButton"
    active="True"
    preferredHeight="175"
    color="#23272a"
    padding="25 0 0 0">
  </Button>

<!-- Table Layout -->
  <TableLayout
    class="generalTable"
    cellBackgroundColor="#23272a"
    rowBackgroundColor="#23272a">
  </TableLayout>

  <TableLayout
    class="columnSplit"
    columnWidths="0 107"
    useGlobalCellPadding="false"
    cellBackgroundColor="clear">
  </TableLayout>

  <TableLayout
    class="zoneTable"
    rowBackgroundColor="#939393"
    cellPadding="5 5 5 5">
  </TableLayout>

<!-- Cell -->
  <Cell
    class="titlesCell"
    overrideGlobalCellPadding="True"
    padding="25 25 0 0">
  </Cell>

  <Cell
    class="checkboxCell"
    overrideGlobalCellPadding="True"
    padding="107 0 0 0">
  </Cell>

  <Cell
    class="deckCountCell"
    overrideGlobalCellPadding="True"
    padding="0 50 0 0">
  </Cell>

  <Cell
    class="zoneCell"
    dontUseTableCellBackground="True"
    color="clear">
  </Cell>

